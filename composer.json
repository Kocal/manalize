{
    "name": "manala/manalize",
    "description": "Lightweight binary providing vagrant stacks provisionned through manala ansible roles",
    "keywords": ["manala", "ansible", "vagrant", "docker"],
    "homepage": "https://github.com/manala/manalize",
    "license": "MIT",
    "authors": [
        {
            "name": "Manala",
            "email": "contact@manala.io",
            "homepage": "https://github.com/manala"
        }
    ],
    "require": {
        "php": ">=7.0",
        "symfony/console": "^3.1",
        "symfony/filesystem": "^3.1",
        "symfony/process": "^3.1",
        "symfony/stopwatch": "^3.1",
        "symfony/yaml": "^3.1",
        "composer/semver": "^1.4",
        "nikic/iter": "^1.4",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "symfony/console": "~3.2@dev",
        "phpunit/phpunit": "~5.4",
        "symfony/phpunit-bridge": "~2.8|~3.0",
        "friendsofphp/php-cs-fixer": "2.x@dev"
    },
    "autoload": {
        "psr-4": { "Manala\\Manalize\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Manala\\Manalize\\Tests\\": "tests/" }
    },
    "bin": ["bin/manalize"]
}
